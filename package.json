    "tippy.js": "^6.2.3",
    "tiptap": "^1.27.1",
    "tiptap-extensions": "^1.29.1",
    "vue-i18n": "^8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@quasar/app": "^1.6.2",
    "@types/node": "^10.17.15",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-eslint": "^10.0.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
