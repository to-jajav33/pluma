{
  "name": "pluma",
  "version": "0.0.1",
  "description": "App for writing stories",
  "productName": "Pluma App",
  "cordovaId": "com.jajav33.pluma.app",
  "capacitorId": "",
  "author": "jajav33 <to.jajav33@gmail.com>",
  "private": true,
  "repository": {
    "url": "https://github.com/to-jajav33/pluma.git"
  },
  "scripts": {
    "git:commit": "npx git-cz",
